{%- for enumeration in enumerations %}
class {{ enumeration.name }}(str, Enum):
    {%- if enumeration.doc %}
    """
    {{ "\n".join(enumeration.doc) }}
    """
    {%- endif -%}
{% for value in enumeration.values %}
    {{ value.upper().replace('.', '_') }} = "{{ value }}"
{%- endfor %}
    @property
    def metadata(self) -> dict:
        return {
            "ili2py": {

            },
            "interlis": {
                "oid": "{{ enumeration.identifier }}",
                "kind": "Enumeration",
                "meta_attributes": {
                    {%- for item in enumeration.meta_attributes %}
                        "{{ item.name }}": "{{ item.value }}",
                    {%- endfor %}
                }
            }
        }
{% endfor -%}
